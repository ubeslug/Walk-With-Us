[sub_resource type="GDScript" id="GDScript_iwjp3"]
script/source = "extends Node2D
const Balloon = preload(\"res://scenes/dialogue balloons/balloon.tscn\")

@export var dialogue_resource: DialogueResource
@export var dialogue_start: String = \"start\"

@export var start_with_items: bool = false

@onready var animation_player = $AnimationPlayer



func _ready():
	if start_with_items:
		InventoryHandler.add_to_inventory(1)
		InventoryHandler.add_to_inventory(5)
	
	var balloon: Node = Balloon.instantiate()
	get_tree().current_scene.add_child(balloon)
	balloon.start(dialogue_resource, dialogue_start)
	
	if InventoryHandler.check_inventory(3) or InventoryHandler.check_inventory(4) or InventoryHandler.check_inventory(5):
		$images/bear_approach.visible = false
	else:
		$images/distant_bear.visible = true



func bear_distant() -> void:
	animation_player.play(\"bear_distant\")

func bear_approach() -> void:
	animation_player.play(\"bear_approach\")

func bear_attack() -> void:
	animation_player.play(\"bear_attack\")

func bear_defense() -> void:
	animation_player.play(\"bear_defense\")

func bear_leave() -> void:
	animation_player.play(\"bear_leave\")

func fade_out() -> void:
	animation_player.play(\"fade-out\")
	await animation_player.animation_finished
	
	get_tree().change_scene_to_file(\"res://scenes/encounters/necrotic_sludge_baby.tscn\")

func death() -> void:
	animation_player.play(\"fade-out\")
	await  animation_player.animation_finished
	
	get_tree().change_scene_to_file(\"res://scenes/menus/gameover.tscn\")
"

